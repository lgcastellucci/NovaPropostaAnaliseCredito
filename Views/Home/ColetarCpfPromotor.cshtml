@model NovaPropostaAnaliseCredito.Models.ProposalModel

<div class="text-center">
    <h1 class="display-4">Coletar CPF do Promotor</h1>
</div>

<form asp-action="SubmitCpfPromotor" method="post">
    <input type="hidden" name="ProposalJson" value='@ViewBag.ProposalJson' />

    <div class="form-group">
        <label for="cpfPromotor">CPF do Promotor</label>
        <input type="text" class="form-control" id="cpfPromotor" name="cpfPromotor" value=@Model.cpfPromotor required>
    </div>
    <div class="form-group">
        <br />
    </div>
    <div class="form-group d-flex justify-content-end">
        <button type="submit" class="btn btn-primary">Enviar</button>
    </div>
</form>
